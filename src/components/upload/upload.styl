.s-upload-wrapper{
  .upload{
    display inline-block
    border 1px solid #e3e3e3
    cursor pointer
    &:hover{
      border-color #20a0ff
    }
    .upload-icon{
      width 200px
      height 200px
      line-height 200px
      text-align center
    }
    .upload-input{
      display none
    }
    .img-area{
      width 200px
    }
  }
}
